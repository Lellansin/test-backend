  "bugs": {
    "url": "https://github.com/Lellansin/test-backend/issues"
  },
  "homepage": "https://github.com/Lellansin/test-backend#readme",
  "devDependencies": {
    "@types/ejs": "^3.1.1",
    "@types/glob": "^7.2.0",
    "@types/ioredis": "^4.28.10",
    "@types/koa": "^2.13.4",
    "@types/koa__router": "^8.0.11",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-session": "^5.10.6",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@types/sinon": "^10.0.11",
    "@types/supertest": "^2.0.12",
    "axios": "^0.27.2",
    "class-validator": "^0.13.2",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "puppeteer": "^15.1.0",
    "sinon": "^14.0.0",
    "supertest": "^6.2.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "why-is-node-running": "^2.2.2"
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "ejs": "^3.1.8",
    "glob": "^8.0.3",
    "ioredis": "^4.28.5",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-session": "^6.2.0",
